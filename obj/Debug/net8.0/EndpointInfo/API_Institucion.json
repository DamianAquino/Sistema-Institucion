{
  "openapi": "3.0.1",
  "info": {
    "title": "API_Institucion",
    "version": "1.0"
  },
  "paths": {
    "/api/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Auth/registrar": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "Rol": {
                    "type": "string"
                  },
                  "Dni": {
                    "type": "string"
                  },
                  "Nombre": {
                    "type": "string"
                  },
                  "Email": {
                    "type": "string"
                  },
                  "Carrera": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string"
                  },
                  "Foto": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Rol": {
                  "style": "form"
                },
                "Dni": {
                  "style": "form"
                },
                "Nombre": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Carrera": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "Foto": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/inicio": {
      "get": {
        "tags": [
          "Usuario"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Login": {
        "type": "object",
        "properties": {
          "dni": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}